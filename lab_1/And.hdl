/**
 * And gate: 
 * out = 1 if (a == 1 and b == 1)
 *       0 otherwise
 */

CHIP And {
    IN a, b;
    OUT out;

    PARTS:
    Not (in=a, out=nota);
    Not (in=b, out=notb);
    Or 	(a=nota, b=notb, out=res);
    Not (in=res, out=out);
    
/**
 * optimized
 * PARTS:
 * Nand (a=a, b=b, out=temp);
 * Not (in=temp, out=out);
 */
}

